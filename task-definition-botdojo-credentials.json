{
  "family": "chatbot-demo-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::839404775617:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "chatbot-demo-container",
      "image": "839404775617.dkr.ecr.us-east-1.amazonaws.com/chatbot-demo:latest",
      "portMappings": [
        {
          "containerPort": 3001,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "PORT",
          "value": "3001"
        },
        {
          "name": "BOTDOJO_API_KEY",
          "value": "d79fa8f7-d776-431e-8fbc-56f47deb79d5"
        },
        {
          "name": "BOTDOJO_BASE_URL",
          "value": "https://api.botdojo.com/api/v1"
        },
        {
          "name": "BOTDOJO_ACCOUNT_ID",
          "value": "afa02f70-4182-11f0-a4dd-cb9f7db4f88a"
        },
        {
          "name": "BOTDOJO_PROJECT_ID",
          "value": "cbeee3b0-4182-11f0-b112-85b41bbb6a74"
        },
        {
          "name": "BOTDOJO_FLOW_ID",
          "value": "596f6181-5c0c-11f0-86ab-7561582ecdb8"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/chatbot-demo",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
